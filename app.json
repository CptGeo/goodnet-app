{
  "expo": {
    "name": "Goodnet",
    "slug": "goodnet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/goodnet_logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.goodnet.cptgeo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
